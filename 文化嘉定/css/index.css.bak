	height:273px;
	float:left;
}

.tJan-right .r1{
	width:328px;
	height:128px;
	background:#e8e8e8;
	overflow:hidden;
	position:relative;
	padding:16px 0 0 40px;
	line-height:22px;
}
.tJan-right .r1 .r1P{
	display:block;
	margin-top:15px;
}
.tJan-right .r1 a{
	color:#000;
}
.tJan-right .r1 a:hover{
	color:#999999;
}
.tJan-right .r1 p{
	color:#999999;
}
.tJan-right .r1 i{
	display:block;
	width:40px;
	height:30px;
	background:#94295b;
	transform:rotate(-45deg);
	position:absolute;
	top:-12px;
	left:-16px;
}

.tJan-right .r2{
	width:328px;
	height:145px;
	background:#e8e3e0;
	padding-top:22px;
}
.tJan-right .r2 a:hover{
	color:#999
}

.tJan-right .r2 li{
	background: url(../img/dot_03.jpg) no-repeat 0px center;
	line-height:27px;
	padding-left:14px;
	margin-left:20px;

}

