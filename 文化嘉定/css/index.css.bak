/*页面标头部分start*/

header {
	width: 100%;
	height: auto;
	position: fixed;
	top: 0;
	z-index:999;
}


/*顶部start*/

header .top {
	height: 43px;
	width: 100%;
	background: #000000;
	font-size: 12px;
}


/*左部分start*/

